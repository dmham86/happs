<% provide(:title, 'Sign Up') %>
<h1>Sign Up</h1>
<p class="strong">
	Already have an account? <%= link_to "Sign in", signin_path %>.
</p>

<%= form_for(@user) do |f| %>
	<%= render 'fields', f: f %>
	<span class="create">
		<%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
	</span>
<% end %>
